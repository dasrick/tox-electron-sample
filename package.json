{
  "name": "tox-electron-sample",
  "productName": "toxApp",
  "version": "0.0.5",
  "description": "Electronized sample application based on AngularJS (frontend) and some more ...",
  "author": "Enrico Hoffmann <dasrick@gmail.com>",
  "license": "MIT",
  "main": "main.js",
  "homepage": "https://github.com/dasrick/tox-electron-sample#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dasrick/tox-electron-sample.git"
  },
  "bugs": {
    "url": "https://github.com/dasrick/tox-electron-sample/issues"
  },
  "electron-version": "0.37.2",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.37.2",
    "electron-release": "^2.2.0",
    "grunt": "^0.4.5",
    "grunt-chmod": "^1.1.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.1.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^1.2.0",
    "grunt-contrib-watch-chokidar": "^1.0.0",
    "grunt-curl": "^2.2.0",
    "grunt-download-electron": "^2.1.2",
    "grunt-electron": "^2.0.1",
    "grunt-electron-installer": "^2.0.0",
    "grunt-newer": "^1.1.2",
    "grunt-rcedit": "^0.4.0",
    "grunt-shell": "^1.2.1",
    "grunt-shell-spawn": "^0.3.10",
    "grunt-webpack": "^1.0.11",
    "load-grunt-tasks": "^3.4.1",
    "publish-release": "^1.2.0",
    "shell-escape": "^0.2.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "start": "grunt",
    "start-dev": "NODE_ENV=development grunt",
    "release": "grunt release",
    "old-start": "node_modules/.bin/electron .",
    "old-clean": "rm -rf ./dist",
    "old-clean:osx": "rm -rf ./dist/osx",
    "old-clean:win": "rm -rf ./dist/win",
    "old-build": "npm run clean && npm run build:osx && npm run build:win",
    "old-build:osx": "npm run clean:osx && electron-packager ./ $npm_package_productName --out=dist/osx --platform=darwin --arch=x64 --app-version=$npm_package_version --version=$npm_package_electron-version --icon=assets/osx/icon.icns",
    "old-build:win": "npm run clean:win && electron-packager ./ $npm_package_productName --out=dist/win --platform=win32 --arch=ia32 --app-version=$npm_package_version --version=$npm_package_electron-version --icon=assets/win/icon.ico",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "angular": "^1.5.1",
    "angular-loading-bar": "^0.9.0",
    "angular-resource": "^1.5.1",
    "angular-sanitize": "^1.5.1",
    "angular-translate": "^2.10.0",
    "angular-translate-loader-partial": "^2.10.0",
    "angular-ui-router": "^0.2.18",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.5.0",
    "npm-font-source-sans-pro": "0.0.3"
  }
}
